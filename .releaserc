{
        "branches": [
                "main",
                "humble"
        ],
        "plugins": [
                [
                        [
                                "@semantic-release/commit-analyzer",
                                {
                                        "preset": "conventionalcommits"
                                }
                        ],
                        {
                                "verifyReleaseCmd": "echo \"next_version=${nextRelease.version}\" >> $GITHUB_OUTPUT"
                        }
                ],
                "@semantic-release/commit-analyzer"
        ]
}